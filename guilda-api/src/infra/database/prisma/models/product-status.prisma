model ProductStatus {
  id      Int        @id @default(autoincrement()) @map("IDGDA_PRODUCT_STATUS")
  status  String?    @map("STATUS")
  createdAt             DateTime               @default(now()) @map("CREATED_AT")
  deletedAt             DateTime?              @map("DELETED_AT")

  @@map("GDA_PRODUCT_STATUS")
}
